import java.util.Scanner;
public class HowManyNumbers{
    public static void main(String [] args){
        Scanner in= new Scanner(System.in);
        int n=0;
        int i=0;
        System.out.println("Enter numbers: ");
        n=in.nextInt();
        while (n!=-1){
            n=in.nextInt();
            i++;
        }
        System.out.println(i+ " numbers were entered.");
        
    }
    
}
